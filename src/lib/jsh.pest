seperator = _{" " | "\t"}
disallowCharacters = _{" " | "|" | "\"" | "\t"}
pathCharacter = _{ !disallowCharacters ~ ANY }
Argument = { pathCharacter+ }
LocalOperation = { "./" ~ pathCharacter+ }
AnyOperation = { !"./" ~ pathCharacter+ }
Operation = { LocalOperation | AnyOperation }
ExecuteCommand = {Operation ~ (seperator+ ~ Argument)* }
CommandComposition = { ExecuteCommand ~ seperator* ~ SerialCommand?}
SerialCommand = { "|" ~ seperator* ~ CommandComposition}
